{
  "name": "Market Analysis - Real-Time Stock Analysis Platform",
  "version": "3.0.0",
  "status": "active",
  "features": {
    "realTimeStockData": true,
    "investmentRecommendations": true,
    "multiCurrency": true,
    "interactiveCharts": true,
    "riskAssessment": true,
    "globalStocks": true,
    "tradingStrategies": ["Long Term", "Options Trading", "Scalping"],
    "supportedCurrencies": ["USD", "INR", "EUR", "GBP", "JPY", "CAD", "AUD"],
    "minimumInvestment": "None",
    "maxSymbolLength": 20,
    "dataSources": ["Grow App", "Yahoo Finance", "Alpha Vantage", "Polygon.io"]
  },
  "lastUpdated": "2024-12-19",
  "deployment": {
    "platform": "Netlify",
    "buildCommand": "npm run build",
    "publishDirectory": "out",
    "nodeVersion": "18"
  }
}